{
  "name": "now-admin",
  "version": "1.0.0",
  "description": "Admin panel for now.sh",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/xo",
    "build": "next build",
    "start": "next start"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "extends": "xo-react",
    "globals": [
      "document"
    ],
    "ignores": [
      ".next/**",
      "node_modules/**",
      "static/**"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": 0
    }
  },
  "keywords": [
    "admin",
    "panel",
    "now"
  ],
  "author": "Mario Colque <colquemario@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "js-cookie": "^2.1.4",
    "next": "^2.3.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "timeago.js": "^3.0.1"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.12.0",
    "eslint-plugin-react": "^7.0.1",
    "xo": "^0.18.2"
  }
}
